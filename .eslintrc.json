{
    "extends": "airbnb-base",

    "rules": {
        "linebreak-style":"off",
        "indent": [
            "error",
            4,
            { "SwitchCase": 1 }
        ],
        "no-undef": "off",
        "comma-dangle": "off",
        "sourceType": "module",
        "no-param-reassign": "off",
        "no-plusplus": "off",
        "max-len" : "off",
        "wrap-iife": "off",
        "class-methods-use-this": "off",
        "no-underscore-dangle": "off",
        "no-restricted-globals": "off",
        "prefer-template": "off",
        "no-unused-expressions": "off",
        "operator-assignment": "off"

    }
}